   "source": [
    "import re\n",
    "\n",
    "with open(\"email.txt\", \"r\") as fh:\n",
    "    data = fh.read()\n",
    "\n",
    "pattern = r\"[a-zA-Z]+[a-zA-Z0-9_.-]+@[a-zA-Z]+\\.[a-zA-Z]+\"\n",
    "\n",
    "matches = re.finditer(pattern, data)\n",
    "\n",
    "for match in matches:\n",
    "    print(match)\n"
   ]
