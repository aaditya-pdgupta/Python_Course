   "id": "b14b7427-6408-40bc-a4fd-e1047484bd74",
   "metadata": {},
   "outputs": [],
   "source": [
    "s1 = \"Hello World\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "8a3fd782-56b6-4547-b938-d1b6dcfe6746",
